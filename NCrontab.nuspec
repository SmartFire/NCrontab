<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>ncrontab</id>
        <title>NCrontab</title>
        <version>2.0.1</version>
        <authors>Atif Aziz</authors>
        <description>NCrontab is crontab for Microsoft .NET Framework 2.0 and above. It provides parsing and formatting of crontab expressions as well as calculation of occurrences of time based on a schedule expressed in the crontab format.</description>
        <language>en-US</language>
        <copyright>Copyright &#169; 2008 Atif Aziz. All rights reserved.</copyright>
        <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
        <projectUrl>http://ncrontab.googlecode.com/</projectUrl>
        <tags>cron schedule time</tags>
        <releaseNotes>Minor release due to project move (no code changes).</releaseNotes>
    </metadata>
    <files>
        <file src="bin\Release\*" target="lib\net20" />
        <file src="COPYING.txt" target="COPYING.txt" />
        <file src="*.cs" target="src" />
        <file src="NCrontab\**\*.cs" target="src\NCrontab" />
    </files>
</package>
